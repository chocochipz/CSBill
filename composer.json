{
    "autoload": {
        "psr-0":         {
            "": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "description": "CSBill : General Billing Management",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-assets-install": "symlink",
        "symfony-web-dir": "web"
    },
    "minimum-stability": "stable",
    "name": "csbill/core",
    "repositories": [
        {
            "package":             {
                "name": "twitter/bootstrap",
                "source":                 {
                    "reference": "v2.1.1",
                    "type": "git",
                    "url": "https://github.com/twitter/bootstrap"
                },
                "version": "master"
            },
            "type": "package"
        }
    ],
    "require": {
        "customscripts/datagrid-bundle": "dev-master",
        "customscripts/menubundle" : "dev-master",
        "doctrine/doctrine-bundle": "1.0.*",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "jms/di-extra-bundle": "1.1.*",
        "jms/security-extra-bundle": "1.2.*",
        "knplabs/knp-menu-bundle": "dev-master",
        "php": ">=5.3.3",
        "sensio/distribution-bundle": "2.1.*",
        "sensio/framework-extra-bundle": "2.1.*",
        "sensio/generator-bundle": "2.1.*",
        "stof/doctrine-extensions-bundle": "dev-master",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/monolog-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.1.*",
        "symfony/symfony": "2.1.*",
        "twig/extensions": "1.0.*",
        "twitter/bootstrap": "master",
        "willdurand/expose-translation-bundle": "dev-master",
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    }
}